{
  "name": "babylonjs-game-starter",
  "version": "1.0.1",
  "description": "Babylonjs game project starter build with webpack",
  "author": "pdl39",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pdl39/babylonjs-game-starter.git"
  },
  "main": "game/index.ts",
  "keywords": [
    "babylonjs",
    "3D",
    "webpack",
    "webgl",
    "webgpu",
    "game",
    "nodejs",
    "expressjs",
    "typescript",
    "javascript",
    "boilerplate",
    "starter"
  ],
  "bin": {
    "babylonjs-game-starter": "bin/bgs.js"
  },
  "html": "src/index.html",
  "fallback": "src/fallback.html",
  "src": "src",
  "assets": "assets",
  "favicon": "assets/favicon/favicon.ico",
  "scripts": {
    "build": "NODE_ENV=production webpack --config webpack.prod.js",
    "server": "node server/server.js",
    "start": "npm run server:webpack & npm run server:dev",
    "start:dev": "npm run build:dev & npm run server:dev",
    "build:dev": "webpack --watch --config webpack.dev.js",
    "server:webpack": "webpack serve --config webpack.dev.js",
    "server:dev": "nodemon server/server.js",
    "server:dev2": "NODE_ENV=development nodemon server/server.js"
  },
  "dependencies": {
    "@babel/runtime": "^7.15.3",
    "express": "^4.17.1",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-typescript": "^7.15.0",
    "@babylonjs/core": "^4.2.0",
    "@babylonjs/gui": "^4.2.0",
    "@babylonjs/inspector": "^4.2.0",
    "@babylonjs/loaders": "^4.2.0",
    "@types/react": "^17.0.17",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "babel-loader": "^8.2.2",
    "css-loader": "^6.2.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "file-loader": "^6.2.0",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.3.2",
    "nodemon": "^2.0.12",
    "prettier": "2.3.2",
    "style-loader": "^3.2.1",
    "ts-loader": "^9.2.5",
    "typescript": "^4.3.5",
    "url-loader": "^4.1.1",
    "webpack": "^5.50.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-middleware": "^5.0.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.8.0"
  },
  "engines": {
    "node": "~14.17.5",
    "npm": "~6.14.14"
  },
  "bugs": {
    "url": "https://github.com/pdl39/babylonjs-game-starter/issues"
  },
  "homepage": "https://github.com/pdl39/babylonjs-game-starter#readme"
}
